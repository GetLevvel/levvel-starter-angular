{
  "name": "levvel-starter-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "ng lint --fix=true",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "concurrently \"wait-on https://localhost:5001 && npm run cypress:runner && npm run killserver\" \"npm run start\"",
    "cypress:runner": "npm run cypress:run || npm run killservererror",
    "killserver": "wmic process where \"name='dotnet.exe'\" delete",
    "killservererror": "npm run killserver && exit 1",
    "prettier": "prettier --write .",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "concurrently": "^5.3.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^6.5.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.2.1"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "browserslist": [
    "defaults",
    "not IE 9-11",
    "maintained node versions"
  ]
}
